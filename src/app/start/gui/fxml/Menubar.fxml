<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>

<MenuBar xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="app.start.gui.GUI">
   <menus>
      <Menu mnemonicParsing="false" text="File">
         <items>
            <MenuItem fx:id="closeButton" mnemonicParsing="false" onAction="#appclosemenubar" onMenuValidation="#appclosemenubar" text="Quit" />
         </items>
      </Menu>
      <Menu mnemonicParsing="false" text="Settings">
         <items>
            <MenuItem fx:id="settingsmenuitem" mnemonicParsing="false" onAction="#showConfigDialog" text="Settings" />
            <MenuItem mnemonicParsing="false" onAction="#nodeDiscoveryStarter" text="Node Discovery" />
         </items>
      </Menu>
      <Menu mnemonicParsing="false" text="Help">
         <items>
            <MenuItem mnemonicParsing="false" text="Manual PDF" />
            <MenuItem mnemonicParsing="false" text="About Us" />
         </items>
      </Menu>
   </menus>
</MenuBar>
